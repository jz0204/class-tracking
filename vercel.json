{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHONASYNCIODEBUG": "1",
        "PYTHONOPTIMIZE": "2"
    }
}
